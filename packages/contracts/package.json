{
  "name": "@owlprotocol/contracts",
  "version": "1.2.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/owlprotocol/contracts.git"
  },
  "homepage": "https://github.com/owlprotocol/contracts.git",
  "copyright": "Copyright 2021 Owl Protocol",
  "license": "MIT",
  "type": "commonjs",
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/esm/index.mjs",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    }
  },
  "files": [
    "dist/*",
    "lib/*",
    "!lib/forge-std",
    "src/*",
    "LICENSE",
    "README.md",
    "contracts/*"
  ],
  "scripts": {
    "build": "npm-run-all compile generate-types docs esbuild tsc build:interfaceIds",
    "build:watch": "npm-run-all -p tsc:watch esbuild:watch",
    "build:interfaceIds": "rm interfaceId/* && node lib/cjs/ethers/generateInterfaceIds.js",
    "docs": "hardhat docgen",
    "esbuild": "node esbuild.config.mjs",
    "esbuild:watch": "ESBUILD_WATCH=true node esbuild.config.mjs",
    "tsc": "mkdir -p lib/types && cp -r src/typechain lib/types && tsc --project tsconfig.build.json",
    "tsc:watch": "mkdir -p lib/types && cp -r src/typechain lib/types && tsc --project tsconfig.build.json -w",
    "hardhat:compile": "hardhat compile",
    "hardhat:node": "hardhat node --no-deploy",
    "ipfs": "jsipfs daemon",
    "ipfs:configure": "jsipfs config --json API.HTTPHeaders.Access-Control-Allow-Methods '[\"PUT\", \"POST\"]' && jsipfs config --json API.HTTPHeaders.Access-Control-Allow-Origin '[\"http://127.0.0.1:5002\", \"http://localhost:3000\", \"http://127.0.0.1:5001\", \"https://webui.ipfs.io\"]'",
    "clean:types": "rimraf types && hardhat clean",
    "coverage": "hardhat coverage --solcoverjs .solcover.js --testfiles 'test/**/*.test.ts'",
    "coverage:gsn": "hardhat --network local coverage --solcoverjs .solcover.js --testfiles 'test/**/*.test.ts'",
    "test": "hardhat test",
    "compile": "hardhat compile",
    "test:parallel": "hardhat test --parallel",
    "test:ERC721Mintable": "hh test lib/cjs/test/assets/ERC721/ERC721Mintable.test.js",
    "generate-types": "npm-run-all -p generate-types:web3 generate-types:ethers",
    "generate-types:web3": "typechain --target web3-v1 --out-dir=src/typechain/web3 'src/artifacts/+(contracts|@openzeppelin|@chainlink|@opengsn)/**/+([a-zA-Z0-9_]).json'",
    "generate-types:ethers": "typechain --target=ethers-v5 --out-dir=src/typechain/ethers 'src/artifacts/+(contracts|@openzeppelin|@chainlink|@opengsn)/**/+([a-zA-Z0-9_]).json'",
    "node": "npm run node:anvil",
    "node:anvil": "anvil",
    "deploy": "npm run deploy:shapes",
    "deploy:beacons": "hardhat deploy --deploy-scripts lib/cjs/deploy-hre --tags UpgradeableBeacon --network anvil",
    "deploy:shapes": "hardhat deploy --deploy-scripts lib/cjs/deploy-hre --tags Shapes --network anvil",
    "hardhat-cjs": "node_modules/hardhat-cjs/bin --version",
    "madge": "madge src/index.ts -c"
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "peerDependencies": {
    "@chainlink/contracts": "^0.4.1",
    "@ethersproject/abi": "^5.4.0",
    "@ethersproject/contracts": "^5.4.1",
    "@ethersproject/providers": "^5.4.3",
    "@opengsn/contracts": "^2.2.6",
    "@openzeppelin/contracts-upgradeable": "^4.7.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/web3-v1": "^3.0.0",
    "ethers": "^5.4.4",
    "typechain": "^5.1.2",
    "web3": "^1.8.0",
    "web3-core": "^1.8.0",
    "web3-eth-contract": "^1.8.0",
    "web3-utils": "^1.8.0"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.4.1",
    "@ethereumjs/tx": "^3.5.2",
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/bignumber": "^5.6.2",
    "@ethersproject/contracts": "^5.6.2",
    "@ethersproject/providers": "^5.5.2",
    "@foundry-rs/hardhat": "^0.1.7",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@opengsn/cli": "^2.2.6",
    "@opengsn/common": "^2.2.6",
    "@opengsn/contracts": "^2.2.6",
    "@opengsn/dev": "^2.2.6",
    "@opengsn/provider": "^2.2.6",
    "@openzeppelin/contracts-upgradeable": "^4.7.0",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@owlprotocol/esbuild-config": "workspace:*",
    "@owlprotocol/nft-sdk": "workspace:*",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@typechain/web3-v1": "^3.1.0",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.4",
    "@types/crypto-js": "^4.1.1",
    "@types/glob": "^7.2.0",
    "@types/lodash": "4.14.188",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.38",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "add": "^2.0.6",
    "bn.js": "^5.2.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-ethers": "^0.0.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^3.4.1",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.8",
    "handlebars": "^4.7.7",
    "hardhat": "2.12.0",
    "hardhat-deploy": "^0.11.10",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-preprocessor": "^0.1.5",
    "hardhat-tracer": "1.1.0-rc.6",
    "ipfs-http-client": "56.0.0",
    "lint-staged": "^11.2.6",
    "madge": "^5.0.1",
    "merkletreejs": "^0.2.31",
    "mocha": "^9.1.4",
    "npm-run-all": "^4.1.5",
    "ow": "^1.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "solc-0.8": "npm:solc@^0.8.12",
    "solidity-coverage": "^0.7.21",
    "solidity-docgen": "^0.6.0-beta.29",
    "truffle": "^5.5.21",
    "ts-node": "^10.8.1",
    "typechain": "^5.2.0",
    "typedoc": "^0.21.10",
    "typedoc-neo-theme": "^1.1.1",
    "typescript": "^4.8.3",
    "uint8arrays": "^3.0.0",
    "web3": "^1.8.0",
    "web3-core": "^1.8.0",
    "web3-eth-contract": "^1.8.0",
    "web3-providers-http": "^1.8.0",
    "web3-utils": "^1.8.0"
  }
}
